<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<connectionStrings>
		<add name="SmartRestaurantConnectionString"
			 connectionString="Data Source=MinionT;Initial Catalog=smart_restaurant;Integrated Security=True;Encrypt=False"
			 providerName="System.Data.SqlClient" />
